<?xml version="1.0" ?>
<PlannerDemoConfig>

  <RRTConnectSolver Name="MySolver">
  </RRTConnectSolver>

  <SamplingProblem Name="MyProblem">

    <PlanningScene>
      <Scene>
        <JointGroup>base</JointGroup>
        <SetRobotDescriptionRosParams>1</SetRobotDescriptionRosParams>
        <Links>
            <Link Name="test" LinkOffset="2 2 2"/>
        </Links>
        <URDF>{hsr123}/resources/hsrb.urdf</URDF>
        <SRDF>{hsr123}/resources/hsrb.srdf</SRDF>
        <LoadScene>{hsr123}/resources/meeting_room_table.scene;</LoadScene>
      </Scene>
    </PlanningScene>

    <Maps>
      <Distance Name="Distance">
        <EndEffector>
            <Frame Link="base_footprint" Base="test"/>
        </EndEffector>
      </Distance>
    </Maps>

    <Inequality>
      <Task Task="Distance" Rho="-1" Goal="0.2"/>
    </Inequality>

    <Goal>1 1 1</Goal>

    <FloatingBaseLowerLimits>-5 -5 0</FloatingBaseLowerLimits>
    <FloatingBaseUpperLimits> 5  5 3.1415</FloatingBaseUpperLimits>
  </SamplingProblem>

</PlannerDemoConfig>